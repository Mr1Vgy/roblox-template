{
  "name": "roblox-template",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Packages": {
        "$path": "Packages"
      },
      "Client" : {
        "$path": "src/client"
      }
    },

    "ServerScriptService": {
      "Source": {
        "$path": "src/server"
      },
    "ServerPackages": {
        "$path": "ServerPackages"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Source": {
          "$path": "src/starterplayer"
        }
      }
    }
  }
}
